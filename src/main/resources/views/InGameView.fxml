<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<HBox xmlns="http://javafx.com/javafx/16"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="fxzone.controller.ingame.InGameUiController$InGameUiControllerFxml"
  prefHeight="400.0" prefWidth="600.0"
  fx:id="hBox"
  maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
  viewOrder="-10"
>

  <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">


    <Pane VBox.vgrow="ALWAYS"/>

    <HBox  HBox.hgrow="ALWAYS" style="-fx-background-color: #282828;" prefHeight="100" visible="false">

    </HBox>

    <HBox spacing="40" style="-fx-background-color: #303030;">
      <VBox>
        <Label contentDisplay="RIGHT" text="INGAMETEST" textFill="WHITE">
          <font>
            <Font size="50.0"/>
          </font>
        </Label>

        <Button graphicTextGap="20.0" mnemonicParsing="false"
          prefWidth="400.0" text="TESTTEST">
          <font>
            <Font size="36.0"/>
          </font>
        </Button>

        <Label contentDisplay="RIGHT" text=" " textFill="WHITE">
          <font>
            <Font size="50.0"/>
          </font>
        </Label>
      </VBox>

      <VBox>
        <Label contentDisplay="RIGHT" text="INGAMETEST2" textFill="WHITE">
          <font>
            <Font size="50.0"/>
          </font>
        </Label>

        <Button graphicTextGap="20.0" mnemonicParsing="false"
          prefWidth="400.0" text="TESTTEST2">
          <font>
            <Font size="36.0"/>
          </font>
        </Button>

        <Label contentDisplay="RIGHT" text=" " textFill="WHITE">
          <font>
            <Font size="50.0"/>
          </font>
        </Label>
      </VBox>
    </HBox>
  </VBox>



</HBox>
